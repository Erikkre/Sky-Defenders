<Defs>
<!-- Start of RulePackDefs -->

 <!-- ==================== Medieval Standard Faction ================= -->
  <RulePackDef>
    <defName>MedTimes_Faction_Society_NameSet</defName>
    <rulePack>
      <rulesStrings>
	<li>name->The Crowned [community]</li>
	<li>name->The Crowned [BadassAnimal]</li>	
	<li>name->Council of the Green [BadassAnimal]</li>	
	<li>name->Kingdom of the [BaddassAnimal]</li>
	<li>name->The [outlander]'s Estates</li>
	<li>name->[family]'s Royal Court</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>outlander</keyword>
          <pathList>
            <li>Words/Nouns/People_Allies</li>
            <li>Words/Nouns/People_Family</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>family</keyword>
          <pathList>
            <li>Words/Nouns/People_Family</li>
          </pathList>
        </li>		
		<li Class="Rule_File">
          <keyword>BadassAnimal</keyword>
          <path>Words/Nouns/Animals_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>community</keyword>
          <path>Words/Nouns/Communities</path>
        </li>		
      </rulesRaw>
    </rulePack>
	
	
 <!-- ==================== Medieval Raider Faction ================= -->	
  </RulePackDef>
    <RulePackDef>
    <defName>MedTimes_Faction_Warband_NameSet</defName>
    <rulePack>
      <rulesStrings>
			<li>name->[badassperson]'s [groupname]</li>
			<li>name->The [badasscolor] [groupname]</li>			
			<li>name->[badassperson]'s [terrainfeature]</li>	
	
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>badassperson</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>groupname</keyword>
          <path>Words/Nouns/GroupNames</path>
        </li>		
        <li Class="Rule_File">
          <keyword>badasscolor</keyword>
          <path>Words/Nouns/Colors_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>		
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
<!-- End of RulePackDefs -->  
</Defs>
