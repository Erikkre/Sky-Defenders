  <!-- ======= Cheese ====== -->
 <!-- (Medieval) Cheese Single --> 
  <RecipeDef>
		<defName>MedTimes_CookMealCheese</defName>
		<label>Create Cheese.</label>
		<description>Cooks a somewhat simple food item from a complex process of adding an acidic, in most cases lemon juice to boiling milk and coagulating milk proteins together. Those proteins are than pressed together in a cloth to create cheese.</description>
		<jobString>Coagulating milk into Cheese.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>	
    <workSkill>Cooking</workSkill>		
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <workAmount>1000</workAmount> 
      <recipeUsers>
        <li>MedTimes_FoodPrepStation</li>	  
        <li>MedTimes_StoneHearth</li>		  	  
      </recipeUsers>	
    <ingredients>
			<li>
				<filter>
          <thingDefs>
            <li>Milk</li>
          </thingDefs>
				</filter>
				<count>0.4</count>
			</li>
		</ingredients>
		<products>
      <MedTimes_MealCheese>1</MedTimes_MealCheese>
    </products>
		<fixedIngredientFilter>
          <thingDefs>
            <li>Milk</li>
          </thingDefs>
		</fixedIngredientFilter>
		<skillRequirements>
				<Cooking>3</Cooking>
		</skillRequirements>
  </RecipeDef>


